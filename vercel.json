{
  "version": 2,
  "builds": [
    {
      "src": "megapredict.html",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/health",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "megapredict.html"
    },
    {
      "src": "/(.*)",
      "dest": "megapredict.html"
    }
  ]
}
